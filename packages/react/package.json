{
  "name": "@lottiefiles/dotlottie-react",
  "version": "0.17.15",
  "type": "module",
  "description": "React wrapper around the dotlottie-web library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LottieFiles/dotlottie-web.git",
    "directory": "packages/react"
  },
  "homepage": "https://github.com/LottieFiles/dotlottie-web#readme",
  "bugs": "https://github.com/LottieFiles/dotlottie-web/issues",
  "author": "LottieFiles",
  "contributors": [
    "Abdelrahman Ashraf <a.theashraf@gmail.com>"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/browser/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "dotlottie",
    "lottie",
    "player",
    "animation",
    "web",
    "canvas",
    "javascript",
    "react",
